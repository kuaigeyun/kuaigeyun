{
  "name": "@univerjs-pro/collaboration-client",
  "version": "0.12.4",
  "private": false,
  "description": "Univer Collaboration Client",
  "author": "DreamNum <developer@univer.ai>",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/univer"
  },
  "homepage": "https://univer.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/dream-num/univer"
  },
  "bugs": {
    "url": "https://github.com/dream-num/univer/issues"
  },
  "keywords": [],
  "exports": {
    ".": {
      "import": "./lib/es/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./*": {
      "import": "./lib/es/*",
      "require": "./lib/cjs/*",
      "types": "./lib/types/index.d.ts"
    },
    "./locale/*": {
      "import": "./lib/es/locale/*.js",
      "require": "./lib/cjs/locale/*.js",
      "types": "./lib/types/locale/*.d.ts"
    },
    "./facade": {
      "import": "./lib/es/facade.js",
      "require": "./lib/cjs/facade.js",
      "types": "./lib/types/facade/index.d.ts"
    },
    "./lib/facade": {
      "import": "./lib/es/facade.js",
      "require": "./lib/cjs/facade.js",
      "types": "./lib/types/facade/index.d.ts"
    },
    "./lib/*": "./lib/*"
  },
  "main": "./lib/es/index.js",
  "types": "./lib/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "crypto-js": "4.2.0",
    "@univerjs-pro/collaboration": "0.12.4",
    "@univerjs-pro/license": "0.12.4",
    "@univerjs/docs": "0.12.4",
    "@univerjs/drawing": "0.12.4",
    "@univerjs/core": "0.12.4",
    "@univerjs/protocol": "0.1.47",
    "@univerjs/network": "0.12.4",
    "@univerjs/sheets": "0.12.4",
    "@univerjs/telemetry": "0.12.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "postcss": "^8.5.6",
    "rxjs": "7.8.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "@univerjs-infra/shared": "0.12.4"
  },
  "scripts": {
    "test": "vitest run --pool=threads",
    "test:watch": "vitest --pool=threads",
    "coverage": "vitest run --coverage --pool=threads",
    "typecheck": "tsc --noEmit",
    "build": "univer-cli build"
  },
  "module": "./lib/es/index.js"
}